        style(type="text/css").
            html,
            body {
                background : #fff;
                font       : normal 0/1.4 'Open Sans', -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen-Sans, Ubuntu, Cantarell, "Helvetica Neue", Helvetica, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
                height     : 100%;
                margin     : 0;
                padding    : 0;
            }
            body {
                border: 1px solid #ddd;
                box-sizing: border-box;
            }
            .carbon-forecast {
                font-size  : 16px;
                display    : inline-block;
                height     : 100%;
                max-height : 20em;
                overflow-x : scroll;
                width      : 100%;
            }
            section {
                font-size  : 0;
                height     : 100%;
            }
            .hour {
                box-sizing     : border-box;
                border         : 2px solid transparent;
                display        : inline-block;
                font-size      : 16px;
                height         : 100%;
                padding        : 0.5em;
                position       : relative;
                width          : 7em;
                vertical-align : top;
            }

            .hour:nth-child(odd) {
                background-color: #eee;
            }

            p {
                font-size  : 14px;
                margin: 0 1em ;
            }
            .timestamp {
                font-size  : 0.8em;
                height     : 5.25em;
                margin     : 0 0 0.25em 0;
                text-align : center;
            }

            time {
                color          : transparent;
                padding-bottom : 1em;
                display        : block;
                text-align     : center;
            }
            time b {
                font-weight : normal;
                display     : block;
                padding-top: 0.2em;
            }
            .icon--house {
                height   : 4em;
                left     : 20%;
                position : relative;
                top      : 5em;
                width    : 60%;
            }

            .icon--plug {
                height         : 1em;
                margin         : -.1em .25em 0 0;
                width          : 1em;
                display        : inline-block;
                vertical-align : middle;
            }
            div,
            date,
            svg,
            svg text,
            svg path {
                transition : all 500ms ease-out;
            }

            .logos {
                text-align: right;
            }

            .logo-link--wwf,
            .logo-link--edf,
            .logo-link--ng {
                margin: 0 1em 0;
                display: inline-block;
                vertical-align: middle;
                height: 30px;
                padding: 0 8px;
            }
            .logo-link--wwf {
                height: 50px;
                margin: -0.5em 1em 0 0;
            }
            .logo-link--ng {
                height: 25px;
            }
            .logo--wwf,
            .logo--edf,
            .logo--ng {
                height: 100%;
                width: auto;
            }
            .logo--edf {
                width: 86px;
            }
            .logo--ng {
                width: 121px;
            }
            .logo--wwf {
                width: 41px;
            }

            .visually-hidden {
                border   : 0 !important;
                clip     : rect(1px 1px 1   px 1px); /* IE6, IE7 */
                clip     : rect(1px, 1px, 1px, 1px);
                height   : 1px !important;
                overflow : hidden;
                padding  : 0 !important;
                position : absolute !important;
                width    : 1px !important;
            }
        script.
            window.ga = window.ga || function() { (ga.q = ga.q || []).push(arguments) };
            ga.l =+ new Date;
            ga('create', 'UA-106816347-2', 'auto', 'nationalGrid');
            ga('create', 'UA-32175-1',     'auto', 'wwf');
            ga('nationalGrid.send', 'pageview');
            ga('wwf.send', 'pageview');
